# Concord Smart Office - Secrets Template
# Copy to secrets.yaml and fill in real values (secrets.yaml is gitignored)

# Spotify Integration (for SpotifyPlus)
spotify_client_id: "your_spotify_client_id"
spotify_client_secret: "your_spotify_client_secret"

# Flipper Zero NFC Bridge
flipper_bridge_token: "your_flipper_bridge_auth_token"

# Database (if using external PostgreSQL)
db_url: "postgresql://user:password@localhost/homeassistant"

# PWA Authentication
pwa_jwt_secret: "your_jwt_secret_for_pwa"
